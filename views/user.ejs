<%- include("partials/header") %>

  <h1><%= user.username %></h1>

  <% polls.forEach(function(poll) { %>
    <div>
      <a href="/poll/<%= poll._id %>">
        <%= poll.created_by.username %> -
          <%= poll.title %> -
            <%= poll.created_at.toLocaleDateString() %>
      </a>
    </div>
    <% }) %>

      <%- include("partials/footer") %>